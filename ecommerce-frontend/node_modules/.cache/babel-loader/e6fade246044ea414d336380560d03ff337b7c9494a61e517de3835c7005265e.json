          </div>\n            <!--            Display order item informations-->\n            <div class=\"col-md-5 px-3\">\n                <div class=\"card-block px-3\">\n                    <h6 class=\"card-title\" >{{orderItem.product.name}}</h6>\n                    <p id=\"item-price\" class=\"mb-0 font-weight-bold\">${{orderItem.product.price}} per unit</p>\n                    <p id=\"item-quantity\" class=\"mb-0\">Quantity : {{orderItem.quantity}}</p>\n                    <p id=\"item-total-price\" class=\"mb-0\">\n                        Total Price : $<span class=\"font-weight-bold\">{{ orderItem.price * orderItem.quantity}}</span>\n                    </p>\n                </div>\n            </div>\n            <div class=\"col-1\"></div>\n        </div>\n\n        <!--        Display the total price-->\n        <div class=\"total-cost pt-2 text-right\">\n            <h5>Total Cost : $ {{order.totalPrice}}</h5>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    name:'OrderItems',\n    props:[\"baseURL\"],\n\n    data() {\n        return {\n            orderItems:[],\n            order: {},\n            token: null,\n            orderID: 0\n        }\n    },\n\n    methods:{\n        getOrder(){\n            // TODO 1. fetch api `/order/${orderID}?token=$token}`\n            // save response into order and order items\n            axios.get(`${this.baseURL}order/${this.orderID}?token=${this.token}`).then((response) => {\n                    if(response.status === 200) {\n                        this.order = response.data\n                        this.orderItems = this.order.orderItems\n                    }\n                },\n                (err)=>{\n                    console.log(err)\n                })\n        }\n    },\n    mounted(){\n        // get the id in param\n        this.orderID = this.$route.params.id;\n        // get the token\n        this.token = localStorage.getItem(\"token\")\n        this.getOrder()\n    }\n}\n\n</script>\n\n<style scoped>\n\nh4 {\n    font-family: 'Roboto', sans-serif;\n    color: #484848;\n    font-weight: 700;\n}\n\n.embed-responsive .card-img-top {\n    object-fit: cover;\n}\n\n</style>\n"],"mappings":"AAuCA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACXC,IAAI,EAAC,YAAY;EACjBC,KAAK,EAAC,CAAC,SAAS,CAAC;EAEjBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,UAAU,EAAC,EAAE;MACbC,KAAK,EAAE,CAAC,CAAC;MACTC,KAAK,EAAE,IAAI;MACXC,OAAO,EAAE;IACb;EACJ,CAAC;EAEDC,OAAO,EAAC;IACJC,QAAQA,CAAA,EAAE;MACN;MACA;MACAT,KAAK,CAACU,GAAG,CAAE,GAAE,IAAI,CAACC,OAAQ,SAAQ,IAAI,CAACJ,OAAQ,UAAS,IAAI,CAACD,KAAM,EAAC,CAAC,CAACM,IAAI,CAAEC,QAAQ,IAAK;QACjF,IAAGA,QAAQ,CAACC,MAAK,KAAM,GAAG,EAAE;UACxB,IAAI,CAACT,KAAI,GAAIQ,QAAQ,CAACV,IAAG;UACzB,IAAI,CAACC,UAAS,GAAI,IAAI,CAACC,KAAK,CAACD,UAAS;QAC1C;MACJ,CAAC,EACAW,GAAG,IAAG;QACHC,OAAO,CAACC,GAAG,CAACF,GAAG;MACnB,CAAC;IACT;EACJ,CAAC;EACDG,OAAOA,CAAA,EAAE;IACL;IACA,IAAI,CAACX,OAAM,GAAI,IAAI,CAACY,MAAM,CAACC,MAAM,CAACC,EAAE;IACpC;IACA,IAAI,CAACf,KAAI,GAAIgB,YAAY,CAACC,OAAO,CAAC,OAAO;IACzC,IAAI,CAACd,QAAQ,EAAC;EAClB;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}