ch categories\n      await axios\n        .get(this.baseURL + \"category/\")\n        .then((res) => (this.categories = res.data))\n        .catch((err) => console.log(err));\n    },\n  },\n  mounted() {\n    this.getCategories();\n  },\n};\n</script>\n\n<style scoped>\nh4 {\n  font-family: \"Roboto\", sans-serif;\n  color: #484848;\n  font-weight: 700;\n}\n\n#add-category {\n  float: right;\n  font-weight: 500;\n}\n</style>\n"],"mappings":"AA2BA,OAAOA,WAAU,MAAO,uCAAuC;AAC/D,IAAIC,KAAI,GAAIC,OAAO,CAAC,OAAO,CAAC;AAC5B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,UAAU,EAAE;IAAEJ;EAAY,CAAC;EAC3BK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,6CAA6C;MACtDC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,aAAaA,CAAA,EAAG;MACpB;MACA,MAAMR,KAAI,CACPS,GAAG,CAAC,IAAI,CAACJ,OAAM,GAAI,WAAW,EAC9BK,IAAI,CAAEC,GAAG,IAAM,IAAI,CAACL,UAAS,GAAIK,GAAG,CAACP,IAAK,EAC1CQ,KAAK,CAAEC,GAAG,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC,CAAC;IACrC;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAACR,aAAa,EAAE;EACtB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}